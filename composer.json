{
    "name": "cevou/behat-screenshot-compare-extension",
    "description": "Screenshot comparison extension for Behat",
    "type": "behat-extension",
    "keywords": ["behat", "bdd", "screenshot", "gui"],
    "homepage": "https://github.com/cevou/BehatScreenshotCompareExtension",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Kraemer",
            "email": "mail@christophkraemer.de"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-imagick": "*",
        "behat/behat": "~3.0",
        "behat/mink-extension": "~2.0@dev",
        "knplabs/gaufrette": "~0.2@dev"
    },
    "require-dev": {
        "behat/mink-selenium2-driver": "~1.0",
        "phpspec/phpspec": "~2.0",
        "bossa/phpspec2-expect": "~1.0",
        "symfony/process": "~2.5"
    },
    "autoload": {
        "psr-0": {
            "Cevou\\Behat\\ScreenshotCompareExtension": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}